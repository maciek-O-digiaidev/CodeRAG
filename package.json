{
  "name": "coderag",
  "private": true,
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "lint": "eslint .",
    "clean": "pnpm -r run clean",
    "benchmark": "pnpm -r --filter @coderag/benchmarks run benchmark",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "eslint": "^9.20.0",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  }
}
